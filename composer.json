{
    "repositories": [
        {
            "type": "git",
            "url": "git@bitbucket.org:eriknyk/phpalchemy.git"
        },
        {
            "type": "package",
            "package": {
                "name": "smarty/smarty",
                "version": "3.1.11",
                "dist": {
                    "url": "http://www.smarty.net/files/Smarty-3.1.11.zip",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": ["libs/"]
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.8.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.8.1.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery-validation",
                "version": "1.10.0",
                "dist": {
                    "url": "http://ajax.aspnetcdn.com/ajax/jquery.validate/1.10.0/jquery.validate.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery-mobile",
                "version": "1.1.1",
                "dist": {
                    "url": "http://code.jquery.com/mobile/1.1.1/jquery.mobile-1.1.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "twitter/bootstrap",
                "version": "2.1.1",
                "dist": {
                    "url": "http://twitter.github.com/bootstrap/assets/bootstrap.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "phpalchemy/phpalchemy": "master",
        "twig/twig"       : "1.10.0",
        "smarty/smarty"   : "3.1.11",
        "jquery/jquery"   : "1.8.1",
        "jquery/jquery-mobile": "1.1.1",
        "jquery/jquery-validation": "1.10.0",
        "twitter/bootstrap": "2.1.1"
    }
}




