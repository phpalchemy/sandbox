{
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/phpalchemy/phpalchemy.git"
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.8.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.8.1.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery-mobile",
                "version": "1.1.1",
                "dist": {
                    "url": "http://code.jquery.com/mobile/1.1.1/jquery.mobile-1.1.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "twitter/bootstrap",
                "version": "2.1.1",
                "dist": {
                    "url": "http://twitter.github.com/bootstrap/assets/bootstrap.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "phpalchemy/phpalchemy": "master",
        "twig/twig"       : "1.12.3",
        "jquery/jquery"   : "1.8.1",
        "jquery/jquery-mobile": "1.1.1",
        "twitter/bootstrap": "2.1.1"
    }
}
